{
  "version": 2,
  "routes": [
    {
      "src":"/api/user",
      "dest": "/src/routes/userRouter.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/products",
      "dest": "/src/routes/productRouter.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    { "src": "/legacy", "status": 404 },
    {
      "src": "/.*",
      "dest": "https://mobile-store-management.vercel.app/login"
    }
  ]
}
